<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#34495E"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/btnback"
        android:layout_width="60dp"
        android:layout_height="37dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:backgroundTint="#34495E"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mywebview"
        app:srcCompat="@drawable/b" />

    <EditText
        android:id="@+id/eturl"
        android:layout_width="300dp"
        android:layout_height="41dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/edittext"
        android:ems="10"
        android:hint="   Search or Enter URL"
        android:inputType="textPersonName"
        android:textColor="#0C0000"
        android:textColorHint="#3E1A1A"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <WebView
        android:id="@+id/mywebview"
        android:layout_width="406dp"
        android:layout_height="594dp"
        android:layout_marginTop="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnlink" />

    <ImageButton
        android:id="@+id/btngo"
        android:layout_width="69dp"
        android:layout_height="35dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/edittext"
        android:backgroundTint="#FFFFFF"
        app:layout_constraintStart_toEndOf="@+id/eturl"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/searchb" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="60dp"
        android:layout_height="37dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="5dp"
        android:backgroundTint="#34495E"
        app:layout_constraintStart_toEndOf="@+id/btnback"
        app:layout_constraintTop_toBottomOf="@+id/mywebview"
        app:srcCompat="@drawable/f" />

    <ImageButton
        android:id="@+id/btnhome"
        android:layout_width="75dp"
        android:layout_height="38dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="2dp"
        android:backgroundTint="#34495E"
        app:layout_constraintStart_toEndOf="@+id/imageButton3"
        app:layout_constraintTop_toBottomOf="@+id/mywebview"
        app:srcCompat="@drawable/h" />

    <ImageButton
        android:id="@+id/btnsetting"
        android:layout_width="60dp"
        android:layout_height="40dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="2dp"
        android:backgroundTint="#34495E"
        app:layout_constraintStart_toEndOf="@+id/btnhome"
        app:layout_constraintTop_toBottomOf="@+id/mywebview"
        app:srcCompat="@drawable/s" />

    <ImageButton
        android:id="@+id/btnmenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="2dp"
        android:backgroundTint="#34495E"
        app:layout_constraintStart_toEndOf="@+id/btnsetting"
        app:layout_constraintTop_toBottomOf="@+id/mywebview"
        app:srcCompat="@drawable/m" />

    <ImageButton
        android:id="@+id/btninsta"
        android:layout_width="54dp"
        android:layout_height="50dp"
        android:layout_marginStart="28dp"
        android:backgroundTint="#34495E"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/eturl"
        app:srcCompat="@drawable/i" />

    <ImageButton
        android:id="@+id/btnfb"
        android:layout_width="54dp"
        android:layout_height="50dp"
        android:layout_marginStart="22dp"
        android:backgroundTint="#34495E"
        app:layout_constraintStart_toEndOf="@+id/btninsta"
        app:layout_constraintTop_toBottomOf="@+id/eturl"
        app:srcCompat="@drawable/fbfb" />

    <ImageButton
        android:id="@+id/btntwit"
        android:layout_width="54dp"
        android:layout_height="50dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="4dp"
        android:backgroundTint="#34495E"
        app:layout_constraintStart_toEndOf="@+id/btngit"
        app:layout_constraintTop_toBottomOf="@+id/btngo"
        app:srcCompat="@drawable/twuiw" />

    <ImageButton
        android:id="@+id/btnlink"
        android:layout_width="54dp"
        android:layout_height="50dp"
        android:layout_marginStart="22dp"
        android:backgroundTint="#34495E"
        app:layout_constraintStart_toEndOf="@+id/btnfb"
        app:layout_constraintTop_toBottomOf="@+id/eturl"
        app:srcCompat="@drawable/link" />

    <ImageButton
        android:id="@+id/btngit"
        android:layout_width="54dp"
        android:layout_height="50dp"
        android:layout_marginStart="22dp"
        android:backgroundTint="#34495E"
        app:layout_constraintStart_toEndOf="@+id/btnlink"
        app:layout_constraintTop_toBottomOf="@+id/eturl"
        app:srcCompat="@drawable/git" />

</androidx.constraintlayout.widget.ConstraintLayout>