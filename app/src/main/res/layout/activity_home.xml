<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#34495E"
    tools:context=".home">

    <View
        android:id="@+id/view"
        android:layout_width="393dp"
        android:layout_height="334dp"
        android:background="@drawable/edittext"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="117dp"
        android:layout_height="31dp"
        android:layout_marginStart="3dp"
        android:text="Instagram"
        android:textAlignment="center"
        android:textColor="#34495E"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/btninstagram" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="117dp"
        android:layout_height="31dp"
        android:layout_marginStart="3dp"
        android:text="Facebook"
        android:textAlignment="center"
        android:textColor="#34495E"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/btnfacebook" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="117dp"
        android:layout_height="31dp"
        android:layout_marginStart="20dp"
        android:text="Twitter"
        android:textAlignment="center"
        android:textColor="#34495E"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toBottomOf="@+id/btntwitter" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="117dp"
        android:layout_height="31dp"
        android:layout_marginStart="3dp"
        android:text="GitHub"
        android:textAlignment="center"
        android:textColor="#34495E"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/btnyoutube" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="117dp"
        android:layout_height="31dp"
        android:text="Youtube"
        android:textAlignment="center"
        android:textColor="#34495E"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/btnyoutube" />

    <ImageButton
        android:id="@+id/btngithub"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="24dp"
        android:backgroundTint="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view"
        app:srcCompat="@drawable/github" />

    <ImageButton
        android:id="@+id/btnlinkdin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="20dp"
        android:backgroundTint="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view"
        app:srcCompat="@drawable/linkdin" />

    <ImageButton
        android:id="@+id/btnyoutube"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:backgroundTint="#FFFFFF"
        app:layout_constraintEnd_toStartOf="@+id/btngithub"
        app:layout_constraintStart_toEndOf="@+id/btnlinkdin"
        app:layout_constraintTop_toTopOf="@+id/view"
        app:srcCompat="@drawable/y" />

    <ImageButton
        android:id="@+id/btntwitter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="4dp"
        android:backgroundTint="#FFFFFF"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/t" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="117dp"
        android:layout_height="31dp"
        android:layout_marginStart="20dp"
        android:text="Linkedin"
        android:textAlignment="center"
        android:textColor="#34495E"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toBottomOf="@+id/btnlinkdin" />

    <ImageButton
        android:id="@+id/btnfacebook"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:backgroundTint="#FFFFFF"
        app:layout_constraintStart_toEndOf="@+id/btntwitter"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:srcCompat="@drawable/ffffffff" />

    <ImageButton
        android:id="@+id/btninstagram"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:backgroundTint="#FFFFFF"
        app:layout_constraintStart_toEndOf="@+id/btnfacebook"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:srcCompat="@drawable/iiiiiii" />

</androidx.constraintlayout.widget.ConstraintLayout>